local Library = loadstring(game:HttpGet('https://raw.githubusercontent.com/RXPERM/Script/main/Ui34'))()

local Window = Library:CreateWindow('Void Hub', 'Blox Fruits | 18', 'Loading..', 'rbxassetid://12246362497', false, 'VisualUIConfigs', 'Nordic Dark')

local Tab = Window:CreateTab('Main', true, 'rbxassetid://3926305904', Vector2.new(484, 44), Vector2.new(36, 36))
local Theme = Window:CreateTab('Theme', true, 'rbxassetid://12337371165', Vector2.new(484, 44), Vector2.new(36, 36))

local Section = Tab:CreateSection('Section')

local Label = Section:CreateLabel('Label')

local Paragraph = Section:CreateParagraph('Paragraph', 'Content')

local Button = Section:CreateButton('Button', function()
    print('Button Pressed')
end)

local Textbox = Section:CreateTextbox('Textbox', 'Input', function(Value)
    print(Value)
end)

local Toggle = Section:CreateToggle('Toggle', true, Color3.fromRGB(0, 125, 255), 0.25, function(Value)
    print(Value)
end)

local ThemeSection = Theme:CreateSection('Customize Theme')

ThemeSection:CreateDropdown('Select Theme', {'Default', 'Lighter', 'Light', 'Light+', 'Discord', 'Red And Black', 'Nordic Dark', 'Nordic Light', 'Purple', 'Sentinel', 'Synapse X', 'Krnl', 'Script-Ware', 'Kiriot'}, 'Default', 0.25, function(value)
_G.Theme = value
Library:ChangeTheme(_G.Theme)
end)

local ColorSection = Theme:CreateSection('Customize Colors')

for Index, CurrentColor in next, Library:ReturnTheme() do
    ColorSection:CreateColorpicker(Index, CurrentColor, 0.25, function(Color)
        Library:ChangeColor(Index, Color)
    end, {true})
end
